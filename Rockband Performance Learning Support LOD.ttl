    dcterms:subject "ロックバンド"@ja, "演奏学習"@ja, "LOD"@ja ;
    dcterms:hasPart
        oa:Shishamo ,
        oa:AshitaMo ,
        oa:Glossary ,
        oa:TechniqueSet ,
        oa:EquipmentSet ,
        oa:ScoreSet ;
    dcterms:references
